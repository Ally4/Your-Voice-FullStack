{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off", 
        "no-unused-vars":"off",
        "consistent-return":"off",
        "prefer-template":"off",
        "object-shorthand":"off",
        "newline-per-chained-call":"off",
        "no-undef":"off",
        "import/no-unresolved":"off",
        "no-path-concat":"off",
        "no-console":"off", 
        "no-shadow":"off",
        "max-len":"off",
        "no-multi-spaces":"off"
    }
}